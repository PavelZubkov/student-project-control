<div class="pteam">
  <h4><small>Список участников проекта</small></h4>
  <div class="wrapper">
    <button type="button" class="btn btn-default pteam__btnInviteMember">Пригласить</button>
    <button type="button" class="btn btn-danger pteam__btnKickMember">Исключить</button>
  </div>
  <table class="table">
    <thead>
      <tr>
        <th>#</th>
        <th>Фамилия</th>
        <th>Имя</th>
        <th>Эл. почта</th>
      </tr>
    </thead>
    <tbody>
      {{#each project.team}}
      <tr>
        <th>{{add @index 1}}</th>
        {{#this}}
        <td>{{firstName}}</td>
        <td>{{lastName}}</td>
        <td>{{email}}</td>
        {{/this}}
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>